{
  "name": "react-interface-guarder-hooks",
  "version": "0.0.0",
  "description": "React hooks toolkit for interface guarder flows (placeholder implementation).",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "hooks",
    "interface",
    "guarder",
    "toolkit"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "target": "es2022",
    "platform": "neutral",
    "external": [
      "react",
      "react-dom"
    ],
    "outDir": "dist"
  }
}
