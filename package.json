{
  "name": "vite-plugin-interface-to-object",
  "version": "0.0.1",
  "description": "Vite plugin for generating interface guards during Vite builds",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "prepare": "pnpm run build",
    "build:all": "pnpm -r run build",
    "lint:all": "pnpm -r run lint"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "typescript": ">=5"
  },
  "dependencies": {
    "@babel/generator": "^7.26.2",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.26.4",
    "@babel/types": "^7.26.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "vite": "^5.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "tsup": {
    "platform": "node",
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "target": "node20",
    "outDir": "dist",
    "external": [
      "vite",
      "typescript"
    ]
  }
}